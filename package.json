{
  "name": "@fantasy955/fantasy-ui-react",
  "version": "0.1.2",
  "private": false,
  "description": "A React UI Library.",
  "homepage": "https://fantasy995.github.io/fantasy-ui-react/",
  "repository": {
    "type": "git",
    "url": "git@github.com:fantasy995/fantasy-ui-react.git"
  },
  "license": "ISC",
  "sideEffects": [
    "dist/*",
    "esm/styles/*",
    "lib/styles/*",
    "*.less"
  ],
  "main": "lib/index.js",
  "module": "esm/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "esm"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:types && gulp",
    "build:site": "rimraf doc-site && dumi build",
    "build:types": "tsc -p tsconfig.build.json && cpr lib esm",
    "clean": "rimraf lib esm dist",
    "commit": "git-cz",
    "deploy:site": "npm run build:site && gh-pages -d doc-site",
    "dev": "dumi dev",
    "new": "plop --plopfile ./scripts/plopfile.ts",
    "pre-commit": "lint-staged",
    "preview:site": "npm run build:site && serve doc-site",
    "release": "ts-node ./scripts/release.ts",
    "start": "dumi dev",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:update": "jest --updateSnapshot",
    "test:watch": "jest --watch",
    "vercel-build:site": "rimraf doc-site && DUMI_ENV=development dumi build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.ts?(x)": [
      "prettier --write",
      "eslint --fix",
      "jest --bail --findRelatedTests"
    ],
    "src/**/*.less": [
      "stylelint --syntax less --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.20.7",
    "@types/prop-types": "^15.7.5"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/eslint-plugin": "^7.19.1",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.3",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@umijs/fabric": "^3.0.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-import": "^1.13.5",
    "babel-plugin-named-asset-import": "^0.3.8",
    "babel-preset-react-app": "^10.0.1",
    "bfj": "^7.0.2",
    "browserslist": "^4.18.1",
    "build-ts": "^3.1.3",
    "camelcase": "^6.2.1",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "classnames": "^2.3.2",
    "commitizen": "^4.2.6",
    "cpr": "^3.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "dumi": "^2.0.6",
    "eslint": "^8.31.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unicorn": "^45.0.2",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.0.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-less": "^5.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "less": "^3.10.3",
    "lint-staged": "^13.1.0",
    "mini-css-extract-plugin": "^2.4.5",
    "plop": "^2.7.6",
    "postcss": "^8.4.4",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.0.1",
    "prettier": "^2.8.1",
    "prompts": "^2.4.2",
    "prop-types": "^15.8.1",
    "react-app-polyfill": "^3.0.0",
    "react-dev-utils": "^12.0.1",
    "react-refresh": "^0.11.0",
    "resolve": "^1.20.0",
    "resolve-url-loader": "^4.0.0",
    "sass-loader": "^12.3.0",
    "semver": "^7.3.5",
    "serve": "^14.1.2",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.0.2",
    "terser-webpack-plugin": "^5.2.5",
    "through2": "^4.0.2",
    "ts-jest": "^24.2.0",
    "user": "^0.0.0",
    "web-vitals": "^2.1.4",
    "webpack": "^5.64.4",
    "webpack-dev-server": "^4.6.0",
    "webpack-manifest-plugin": "^4.0.2",
    "workbox-webpack-plugin": "^6.4.1"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "authors": {
    "name": "fantasy955",
    "email": "1642063542@qq.com"
  }
}
